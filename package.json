{
  "name": "booklo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "@opennextjs/cloudflare",
    "pages:build:traditional": "@cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "preview:traditional": "npm run pages:build:traditional && wrangler pages dev .vercel/output/static --compatibility-flag nodejs_compat",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "cf:typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "jest --selectProjects ui",
    "test:e2e": "cd apps/booklo-e2e && npm test",
    "test:e2e:headless": "cd apps/booklo-e2e && npm run test:headless",
    "test:e2e:headed": "cd apps/booklo-e2e && npm run test:headed",
    "test:e2e:debug": "cd apps/booklo-e2e && npm run test:debug",
    "test:e2e:setup": "cd apps/booklo-e2e && npm run setup",
    "test:e2e:watch": "cd apps/booklo-e2e && npm run test:watch",
    "test:e2e:watch:headed": "cd apps/booklo-e2e && npm run test:watch:headed"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.8",
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-query-devtools": "^5.79.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "openai": "^5.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.0.0",
    "@cucumber/cucumber": "^11.3.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@eslint/eslintrc": "^3",
    "@opennextjs/cloudflare": "^1.0.0-beta",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.8.3",
    "wrangler": "^3.84.0"
  }
}
